{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Localization": {
    "SupportedCultures": [ "en", "ru" ],

    "DefaultCulture": "en",
    "DefaultUICulture": "en"
  },

  "Security": {
    "SaltSize": 256,
    "HashSize": 256,
    "HashAlgorithm": "SHA256",
    "HashIterationsCount": 5000
  },

  "AllowedHosts": "*",
  "ServiceName":  "Tr1ppy.NetflixAnalog"
}
